<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	  <TargetFrameworks>net6.0;net7.0;net8.0</TargetFrameworks>
    <ImplicitUsings>enable</ImplicitUsings>
	  <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	  <Version>0.1.1</Version>
	  <Authors>Sergi Ortiz Gomez</Authors>
	  <Company>Digital Door</Company>
	  <Description>Basic Simple ORM objects.</Description>
	  <Copyright>Free</Copyright>
	  <PackageTags>database</PackageTags>
	  <PackageReleaseNotes>Only tested with MS-SQL.</PackageReleaseNotes>
	  <RepositoryUrl>https://github.com/drualcman/DigitalDoor.Database.ADO</RepositoryUrl>
	  <RepositoryType>Git</RepositoryType>
	  <PackageProjectUrl>https://github.com/drualcman/DigitalDoor.Database.ADO</PackageProjectUrl>
	  <ErrorReport>send</ErrorReport>
	  <Nullable>disable</Nullable>
	  <Title>DigitalDoor.Database.Entities</Title>
	  <PackageReadmeFile>readme.md</PackageReadmeFile>
	  <PackageLicenseFile>License.txt</PackageLicenseFile>
  </PropertyGroup>

	<ItemGroup>
		<None Include="License.txt" Pack="true" PackagePath="" />
		<None Include="readme.md" Pack="true" PackagePath="" />
	</ItemGroup>


</Project>
